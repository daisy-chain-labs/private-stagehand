// These were very specific tsconfigs for the fern-sdk/typescript package.
// You may want to reconsider making changes to them.
{
  "compilerOptions": {
    "extendedDiagnostics": true,
    "strict": true,
    "target": "esnext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src"
  },
  "include": ["src"],
  "exclude": []
}
